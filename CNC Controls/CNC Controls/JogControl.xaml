<UserControl x:Class="CNC.Controls.JogControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CNC.Controls"
             mc:Ignorable="d"
             FocusVisualStyle="{x:Null}">
    <GroupBox x:Uid="grp_uijog" Header="Jogging" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="Auto" Height="Auto">
        <local:JogBaseControl x:Name="jogBase" Margin="0,0,10,0" Width="375"></local:JogBaseControl>
    </GroupBox>
</UserControl>
