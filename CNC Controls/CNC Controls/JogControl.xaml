<UserControl x:Class="CNC.Controls.JogControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CNC.Controls"
             mc:Ignorable="d"
             FocusVisualStyle="{x:Null}"
             d:DesignHeight="Auto" MinHeight="275" Width="Auto">
    <GroupBox x:Uid="grp_uijog" Header="Jogging" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="Auto" Height="350">
        <local:JogBaseControl x:Name="jogBase" RenderTransformOrigin="0.5,0.5" Margin="0,0,0,0" Width="588">
            <local:JogBaseControl.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.068"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.591"/>
                </TransformGroup>
            </local:JogBaseControl.RenderTransform>
        </local:JogBaseControl>
    </GroupBox>
</UserControl>
