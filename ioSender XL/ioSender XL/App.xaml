<Application x:Class="GCode_Sender.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GCode_Sender"
             xmlns:properties="clr-namespace:GCode_Sender.Properties"
             xmlns:controls="clr-namespace:CNC.Controls;assembly=CNC.Controls.WPF"
             xmlns:gCode="clr-namespace:CNC.GCode;assembly=CNC.Core"
             xmlns:core="clr-namespace:CNC.Core;assembly=CNC.Core"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary x:Key="ResourceDictionary">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="ThemeDictionary">
                    <ResourceDictionary.MergedDictionaries>
                        <ResourceDictionary Source="\Themes\ThemeSelector.xaml"/>
                    </ResourceDictionary.MergedDictionaries>
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
    </Application.Resources>
</Application>
